<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="task-overview-edit-flow">
    <template-reference>
      <document>/WEB-INF/xdm-task-flow-template.xml</document>
      <id>xdm-task-flow-template</id>
    </template-reference>
    <default-activity>taskOverviewEdit</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>buttonPressed</managed-bean-name>
      <managed-bean-class>com.dmcsol.xdm.taskmgr.view.BackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>Backing</managed-bean-name>
      <managed-bean-class>com.dmcsol.xdm.taskmgr.view.BackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="taskOverviewEdit">
      <page>/taskOverviewEdit.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
