       XdmTasksEO.PAX, 
       XdmTasksEO.ELEM_KEY, 
       XdmTasksEO.REMARKS"
  FromList="XDM_TASKS XdmTasksEO"
  Where="(:pResponsible is null or PERS_ID = :pResponsible) 
and (:pProgramme is null or PROG_ID = :pProgramme) 
and (:pText is null or upper(TEXT) like '%' || upper(:pText) ||  
'%')">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.dmcsol.xdm.taskmgr.model.view.TaskVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pResponsible"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pProgramme"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pText"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="XdmTasksEO"
    Entity="com.dmcsol.xdm.model.entity.XdmTasksEO"/>
  <ViewAttribute
    Name="StartDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="StartDate"
    EntityUsage="XdmTasksEO"
    AliasName="START_DATE"/>
  <ViewAttribute
    Name="TaskId"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="TaskId"
    EntityUsage="XdmTasksEO"
    AliasName="TASK_ID"/>
  <ViewAttribute
    Name="Text"
    PrecisionRule="true"
    EntityAttrName="Text"
    EntityUsage="XdmTasksEO"
    AliasName="TEXT"/>
  <ViewAttribute
    Name="StartWhere"
    PrecisionRule="true"
    EntityAttrName="StartWhere"
    EntityUsage="XdmTasksEO"
    AliasName="START_WHERE"/>
  <ViewAttribute
    Name="FlightNo"
    PrecisionRule="true"
    EntityAttrName="FlightNo"
    EntityUsage="XdmTasksEO"
    AliasName="FLIGHT_NO"/>
  <ViewAttribute
    Name="EndDate"
    PrecisionRule="true"
    EntityAttrName="EndDate"
    EntityUsage="XdmTasksEO"
    AliasName="END_DATE"/>
  <ViewAttribute
    Name="EndWhere"
    PrecisionRule="true"
    EntityAttrName="EndWhere"
    EntityUsage="XdmTasksEO"
    AliasName="END_WHERE"/>
  <ViewAttribute
    Name="Pax"
    PrecisionRule="true"
    EntityAttrName="Pax"
    EntityUsage="XdmTasksEO"
    AliasName="PAX"/>
  <ViewAttribute
    Name="ElemKey"
    PrecisionRule="true"
    EntityAttrName="ElemKey"
    EntityUsage="XdmTasksEO"
    AliasName="ELEM_KEY"/>
  <ViewAttribute
    Name="Remarks"
    PrecisionRule="true"
    EntityAttrName="Remarks"
    EntityUsage="XdmTasksEO"
    AliasName="REMARKS"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.dmcsol.xdm.taskmgr.model.TaskMgrModelBundle"/>
  </ResourceBundle>
</ViewObject>
